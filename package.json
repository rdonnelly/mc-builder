{
  "name": "MCBuilder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:emu": "emulator -avd Pixel_3_API_27 -netdelay none -netspeed full",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "iphone": "react-native run-ios --simulator='iPhone 11 Pro'",
    "ipad": "react-native run-ios --simulator='iPad Pro (11-inch) (2nd generation)'",
    "se": "react-native run-ios --simulator='iPhone SE'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts": "tsc --noEmit --project tsconfig.json",
    "build:pod": "scripts/build.sh podInstall",
    "build:push": "scripts/build.sh push",
    "build:getVersion": "scripts/build.sh getVersion",
    "build:setVersion": "scripts/build.sh setVersion",
    "generateTypes": "ts-node scripts/generateTypes.ts",
    "postinstall": "patch-package && yarn generateTypes"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-navigation/bottom-tabs": "^5.0.0-alpha.34",
    "@react-navigation/native": "^5.0.0-alpha.24",
    "@react-navigation/stack": "^5.0.0-alpha.58",
    "@reduxjs/toolkit": "^1.2.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.5",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^8.0.0",
    "bugsnag-react-native": "^2.23.7",
    "dts-gen": "^0.6.0",
    "lodash": "^4.17.15",
    "marvelsdb-json-data": "https://github.com/rdonnelly/marvelsdb-json-data.git",
    "memoize-one": "^5.1.1",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-device-info": "^6.0.2",
    "react-native-dotenv": "^2.4.1",
    "react-native-flipper": "^0.60.0",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-get-random-values": "^1.3.0",
    "react-native-haptic-feedback": "^1.9.0",
    "react-native-inappbrowser-reborn": "^3.3.4",
    "react-native-reanimated": "^1.4.0",
    "react-native-render-html": "^4.2.0",
    "react-native-safe-area-context": "^3.0.2",
    "react-native-screens": "^2.0.0-alpha.23",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^10.2.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "rn-fetch-blob": "^0.12.0",
    "styled-components": "^5.2.0",
    "ts-node": "^9.0.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.149",
    "@types/react-native": "^0.62.2",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.9.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "16.11.0",
    "reactotron-react-native": "^5.0.0",
    "typescript": "^4.0.2",
    "typescript-plugin-styled-components": "^1.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
