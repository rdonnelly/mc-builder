{
  "name": "@mc-builder/mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android:emu": "emulator -avd Pixel_3a_API_29 -netdelay none -netspeed full",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts": "tsc --noEmit --project tsconfig.json",
    "dev:android": "react-native run-android --no-packager",
    "dev:android:release": "react-native run-android --variant=release --configuration Release",
    "dev:ios": "scripts/build.sh podInstall && react-native run-ios --simulator='iPhone 13 mini' --no-packager",
    "dev:ios:release": "scripts/build.sh podInstall && react-native run-ios --simulator='iPhone 13 mini' --variant=release --configuration Release",
    "build:clean": "scripts/build.sh clean",
    "build:android": "scripts/build.sh android",
    "build:ios": "scripts/build.sh ios",
    "build:getVersion": "scripts/build.sh getVersion",
    "build:setVersion": "scripts/build.sh setVersion"
  },
  "dependencies": {
    "@bugsnag/react-native": "^7.18.2",
