{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:emu": "emulator -avd Pixel_3_API_27 -netdelay none -netspeed full",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios --simulator='iPhone 12 mini'",
    "ipad": "react-native run-ios --simulator='iPad Pro (11-inch) (2nd generation)'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts": "tsc --noEmit --project tsconfig.json",
    "build:pod": "scripts/build.sh podInstall",
    "build:android": "scripts/build.sh android",
    "build:ios": "scripts/build.sh push",
    "build:getVersion": "scripts/build.sh getVersion",
    "build:setVersion": "scripts/build.sh setVersion"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-navigation/bottom-tabs": "^5.11.3",
    "@react-navigation/native": "^5.9.0",
    "@react-navigation/stack": "^5.13.0",
    "@expo/react-native-action-sheet": "^3.8.0",
    "@react-native-community/clipboard": "^1.5.0",
    "@react-native-community/masked-view": "^0.1.5",
    "bugsnag-react-native": "^2.23.7",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-app-auth": "^6.0.0",
    "react-native-appearance": "^0.3.4",
    "react-native-bootsplash": "^3.1.2",
    "react-native-dotenv": "^2.5.0",
    "react-native-flipper": "^0.80.0",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-get-random-values": "^1.3.0",
    "react-native-inappbrowser-reborn": "^3.3.4",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^3.0.2",
    "react-native-screens": "2.17.1",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-webview": "^11.0.1",
    "rn-fetch-blob": "^0.12.0",
    "shared": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.63.23",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components-react-native": "^5.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
