    "@bugsnag/source-maps": "^2.3.1",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.69.0",
    "@types/react-native-sqlite-storage": "^5.0.2",
    "@types/react-native-vector-icons": "^6.4.12",
