{
  "name": "@mc-builder/mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts": "tsc --noEmit --project tsconfig.json",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "emu:android": "emulator -avd Pixel_3a_API_29 -netdelay none -netspeed full",
    "emu:ios": "open -a Simulator --args -CurrentDeviceUDID 6FA59304-F6C2-4670-AE54-A43BDF42293A",
    "dev:android": "react-native run-android --no-packager",
    "dev:android:release": "react-native run-android --mode release --no-packager",
    "dev:ios": "scripts/build.sh podInstall && react-native run-ios --simulator='iPhone 13 mini' --no-packager",
    "dev:ios:release": "scripts/build.sh podInstall && react-native run-ios --simulator='iPhone 13 mini' --configuration Release",
    "build:push": "scripts/build.sh push",
    "clean": "scripts/build.sh cleanAndroid && scripts/build.sh cleanXcode"
  },
  "dependencies": {
    "@bugsnag/react-native": "^7.21.0",
    "@expo/react-native-action-sheet": "^4.0.0",
    "@mc-builder/shared": "1.0.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-clipboard/clipboard": "^1.12.1",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.9",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@react-navigation/stack": "^6.3.18",
    "@reduxjs/toolkit": "^1.9.6",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-bootsplash": "^5.0.3",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-get-random-values": "^1.9.0",
    "react-native-haptic-feedback": "^1.14.0",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-render-html": "^5.1.0",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-webview": "^13.6.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "rn-fetch-blob": "^0.12.0",
    "squel": "^5.13.0",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.20",
    "@babel/runtime": "^7.20.0",
    "@bugsnag/source-maps": "^2.3.1",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.2.1",
    "@types/react-native-sqlite-storage": "^6.0.1",
    "@types/react-native-vector-icons": "^6.4.15",
    "@types/react-redux": "^7.1.27",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.2.3",
    "babel-jest": "^29.2.1",
